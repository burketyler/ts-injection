{
  "name": "ts-injection",
  "version": "1.0.0",
  "description": "Dependency injection framework for Typescript and Javascript projects.",
  "main": "index.js",
  "repository": "https://github.com/burketyler/ts-injection",
  "author": "Tyler Burke",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .build",
    "build": "yarn clean && tsc",
    "release": "yarn prettier && yarn tslint && yarn build",
    "test": "jest",
    "cm": "git-cz",
    "publish": "yarn release && lerna publish"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "commitizen": "^4.2.3",
    "commitlint": "^12.0.1",
    "cz-customizable": "^6.3.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
