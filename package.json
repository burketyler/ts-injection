{
  "name": "ts-injection",
  "version": "3.0.1",
  "description": "Dependency injection framework for Typescript/Javascript NodeJS applications.",
  "keywords": [
    "typescript",
    "di",
    "dependency injection",
    "injection",
    "nodejs",
    "decorators",
    "annotations"
  ],
  "main": ".build/index.js",
  "types": ".build/index.d.ts",
  "repository": "https://github.com/burketyler/ts-injection",
  "author": "Tyler Burke",
  "license": "MIT",
  "files": [
    ".build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prettier:check": "prettier --check src/**/*.ts --loglevel error",
    "prettier:fix": "prettier --write src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "code:check": "yarn prettier:check && yarn lint:check",
    "code:fix": "yarn prettier:fix && yarn lint:fix",
    "clean": "rm -rf .build",
    "build": "yarn clean && tsc",
    "test": "jest",
    "precommit": "yarn code:fix",
    "docs": "(cd docs && yarn start)",
    "cm": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "commitizen": "4.2.4",
    "commitlint": "16.0.1",
    "cz-customizable": "6.3.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "semantic-release": "18.0.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  }
}
