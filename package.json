{
  "name": "ts-injection",
  "version": "1.1.4",
  "description": "Dependency injection framework for Typescript/Javascript NodeJS applications.",
  "main": ".build/index.js",
  "types": ".build/index.d.ts",
  "repository": "https://github.com/burketyler/ts-injection",
  "author": "Tyler Burke",
  "license": "MIT",
  "files": [
    ".build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf .build",
    "build": "yarn clean && tsc",
    "prettier": "prettier --write \"**/*.ts\" --loglevel error",
    "tslint": "tslint -c ./tslint.json src/**/*.ts",
    "release": "yarn prettier && yarn tslint && yarn build",
    "test": "jest",
    "cm": "git-cz"
  },
  "keywords": [
    "typescript",
    "di",
    "dependency injection",
    "injection",
    "nodejs"
  ],
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "commitizen": "^4.2.3",
    "commitlint": "^12.0.1",
    "cz-customizable": "^6.3.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
