{
  "extends": "tslint:recommended",
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "no-console": {
      "severity": "warning",
      "options": ["info", "log", "time", "timeEnd", "trace"]
    }
  }
}
